{
  "public": ".",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/plugins", "destination": "/plugins.html" },
    { "source": "/about", "destination": "/about.html" },
    { "source": "/plugin-detail", "destination": "/plugin-detail.html" }
  ],
  "headers": [
    {
      "source": "**/*.@(json|js|css|png|jpg|jpeg|gif|svg|ico|html)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
